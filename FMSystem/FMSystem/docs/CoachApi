//生成数据上下文实例
Initialize
//
input:
{
   “serviceProvider”: IserviceProvider
}
output:
{
   “context”: fmsContext
}


//用于通过教练的ID来得到指定的教练信息
GetCoachesById
//
input:
{
   “Id”:int, // Id > 0,
}
output:
{
  “Id”: int
  “Name”: string
  “PhoneNo”: long
  “Email”: string
}
ifnotfound:
{
   return null;
}

//通过名字获得指定的教练信息
GetCoachesByName
//
input:
{
   “Name”: string
}
output:
{
  “Id”: int
  “Name”: string
  “PhoneNo”: long
  “Email”: string

}
ifnotfound:
{
   return null;
}


