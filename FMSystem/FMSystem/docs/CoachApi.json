//用于通过教练的ID来得到指定的教练信息
 GetCoachesById
//
  input:
  {
        "Id": 1, // Id > 0,
  }
  output:
  {
         "Code": 200,
         "Message": "成功",
         "Data":
         {
              "Id": 1,
              "Name": "教练",
              "PhoneNo": 1,
              "Email": "123456@qq.com",
              "User":{
                    "UserId": 123,
                    "isCoach": true/false
                    }
          },
          
          "Code":999,
          "Message": "失败",
          "Data": null
               
   }

//通过名字获得指定的教练信息
 GetCoachesByName
//
  input:
  {
         "Name": "教练"
  }
  output:
  {
         "Code": 200,
         "Message": "成功",
         "Data":[
         {
              "Id": 1, 
              "Name": "教练",
              "PhoneNo": 123456,
              "Email": "123456@qq.com",
              "User":{
                    "UserId": 123,
                    "isCoach": true/false
                    }
         }
        
         {
              "Id": 2, 
              "Name": "教练",
              "PhoneNo": 456789,
              "Email": "456789@qq.com",
              "User":{
                    "UserId": 123,
                    "isCoach": true/false
                    }            
         }
            
        ],
        
        "Code": 999,
        "Message": "失败",
        "Data":null

}



